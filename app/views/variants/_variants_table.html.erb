<table>
  <tr>
    <th>Code</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
<% if variants!=nil%>
<% variants.each do |variant| %>
  <tr>
    <td><%= variant.code %></td>
    <td><%= link_to 'Show', product_variant_path(variant.product,variant) %></td>
    <td><%= link_to 'Edit', edit_product_variant_path(variant.product,variant) %></td>
    <td><%= link_to 'Destroy', product_variant_path(variant.product,variant), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
<% end %>
</table>